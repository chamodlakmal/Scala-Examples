package First
// Part - 41 List ---> Part - 2
object Demo1 {
  def main(args:Array[String])
  {
     var colors:List[String]=List("Red","Blue","White")
     println(colors)
     var numbers:List[Int]=List(10,20,30)
     for(num <-numbers)
     {
       println(num)
     }
     colors.foreach(println)
     var sum=0
     numbers.foreach(sum+=_)
     println("Sum ="+sum)
     println(numbers.contains(10.5))
     println(numbers.last)
     println(numbers.sum)
  }
}