package First
// Part - 40 List ---> Part - 1
object Demo1 {
  def main(args:Array[String])
  {
     var colors:List[String]=List("Red","Blue","White")
     println(colors)
     var numbers:List[Int]=List(10,20,30)
     for(num <-numbers)
     {
       println(num)
     }
     colors.foreach(println(_))
  }
}