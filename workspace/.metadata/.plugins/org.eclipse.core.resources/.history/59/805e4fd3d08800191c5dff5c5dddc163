

object Two {
  def main(args:Array[String])
  {
    var x="I love ucsc";
    var length=x.length();
    for(j<-0 until length)
    {
      if(x.charAt(j)=='a'||x.charAt(j)=='e'||x.charAt(j)=='i'||x.charAt(j)=='o'||x.charAt(j)=='u')
      {
        vowelCount+=1;
        x.charAt(j) match {
          case 'a' => a+=1
          case 'e' => e+=1
          case 'i' => i+=1
          case 'o' => o+=1
          case 'u' => u+=1
          case _ =>
        }
        
      }
    }
  }
}