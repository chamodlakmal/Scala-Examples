package First

object Demo {
    
    def main(args:Array[String])
    {    
        var result=(a:Int,b:Int)=>a+b
        var result1=(_:Int)+(_:Int)
        //println(result(10,10))
        //println(result1(2,3))
        val x=math(5,9,10,(x,y)=>x * y)
        println(x)
    }
    
    def math(x:Double,y:Double,z:Double,f:(Double,Double)=>Double):Double=
    {
      return f(f(x,y),z)
    }
    
    
}